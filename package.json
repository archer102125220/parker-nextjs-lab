{
  "name": "parker-nextjs-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sequelize": "./node_modules/.bin/sequelize",
    "sequelize:ts": "ts-node ./node_modules/.bin/sequelize",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack  -H 0.0.0.0 -p 3001",
    "dev-https": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack --experimental-https -H 0.0.0.0 -p 3000",
    "build": "next build",
    "start": "cross-env NITRO_PORT=3001 next start",
    "lint": "next lint",
    "create-i18n": "babel-node ./i18n/google-sheet-to-json.mjs",
    "stress": "ts-node ./stress-testing.ts",
    "initDB": "yarn sequelize:ts db:drop && yarn sequelize:ts db:create && yarn sequelize:ts db:migrate && yarn sequelize:ts db:seed:all",
    "createDB": "yarn sequelize:ts db:create",
    "dropDB": "yarn sequelize:ts db:drop",
    "migrate": "yarn sequelize:ts db:migrate",
    "migrate:undo": "yarn sequelize:ts db:migrate:undo",
    "seed": "yarn sequelize:ts db:seed",
    "seedAll": "yarn sequelize:ts db:seed:all",
    "prepare": "husky install"
  },
